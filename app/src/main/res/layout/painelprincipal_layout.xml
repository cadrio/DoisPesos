<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:id="@+id/linearLayout"
    android:layout_width="match_parent"
    android:layout_height="match_parent">


    <Button
        android:id="@+id/painelprincipal_botaoMassa"
        android:layout_width="200dp"
        android:layout_height="90dp"
        android:layout_marginTop="80dp"

        android:text="MASSA"
        app:layout_constraintBottom_toTopOf="@+id/painelprincipal_botaoVolume"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <Button
        android:id="@+id/painelprincipal_botaoVolume"
        android:layout_width="200dp"
        android:layout_height="90dp"
        android:layout_marginTop="30dp"
        android:text="VOLUME"
        app:layout_constraintTop_toBottomOf="@id/painelprincipal_botaoMassa"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        tools:layout_editor_absoluteY="248dp" />

</androidx.constraintlayout.widget.ConstraintLayout>